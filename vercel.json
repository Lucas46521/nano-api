{
  "version": 2,
  "builds": [
    {
      "src": "api/image.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/image.js"
    },
    {
      "src": "/api",
      "dest": "/api/image.js"
    },
    {
      "src": "/api/perfil",
      "dest": "/api/image.js"
    }
  ]
}